{
  "name": "ecom",
  "version": "0.1.0",
  "private": true,
  "homepage": "http://ryanskystack.github.io/ecom",
  "dependencies": {
    "@reduxjs/toolkit": "^2.2.1",
    "@testing-library/jest-dom": "^5.17.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.89",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@types/react-redux": "^7.1.33",
    "@types/styled-components": "^5.1.34",
    "axios": "^1.6.8",
    "dotenv": "^16.4.5",
    "json-server": "^1.0.0-alpha.23",
    "react": "^18.2.0",
    "react-app-rewired": "^2.2.1",
    "react-dom": "^18.2.0",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.22.3",
    "react-scripts": "5.0.1",
    "redux": "^5.0.1",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "redux-persist-expire": "^1.1.1",
    "redux-saga": "^1.3.0",
    "redux-saga-test-plan": "^4.0.6",
    "redux-thunk": "^3.1.0",
    "styled-components": "^6.1.8",
    "typed-redux-saga": "^1.5.0",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "node --expose-gc ./node_modules/jest/bin/jest.js --runInBand --coverage",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "eject": "react-scripts eject",
    "json-server": "json-server --watch src/assets/shop-data.json --port 3001"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "babel-jest"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga|css)$": "<rootDir>/__mocks__/fileMock.js"
    },
    "setupFiles": [
      "./jest.setup.js"
    ],
    "testEnvironment": "jsdom-sixteen",
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom/extend-expect"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react",
      "@babel/preset-typescript"
    ]
  },
  "devDependencies": {
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/react-test-renderer": "^18.0.7",
    "@types/redux-logger": "^3.0.13",
    "babel-jest": "^29.7.0",
    "babel-plugin-macros": "^3.1.0",
    "gh-pages": "^6.1.1",
    "jest": "^27.5.1",
    "jest-environment-jsdom-sixteen": "^2.0.0",
    "jest-extended": "^4.0.2",
    "react-test-renderer": "^18.2.0"
  }
}